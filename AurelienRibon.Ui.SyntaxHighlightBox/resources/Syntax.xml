<?xml version="1.0" encoding="utf-8" ?>
<Syntax name="Syntax">
  <HighlightWordsRule name="Block">
    <Words>
      TAB SELECTION true false
    </Words>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#0000FF</Foreground>
    <FontWeight>Bold</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightWordsRule>
  <HighlightWordsRule name="Element">
    <Words>
      BGM Chara Background Face Content
    </Words>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#2B91AF</Foreground>
    <FontWeight>Bold</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightWordsRule>
  <HighlightWordsRule name="Att Name">
    <Words>
      Path Position_X Position_Y Name Value Method Title Time Voice Deep NextFile MultipleLanguage TextLayoutBackground ImageWidth ImageHeight FontSize DefaultLanguage
    </Words>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#4EC9B0</Foreground>
    <FontWeight>Bold</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightWordsRule>
  <HighlightLineRule name="Comments">
    <LineStart>//</LineStart>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#008000</Foreground>
    <FontWeight>Normal</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightLineRule>
  <HighlightLineRule name="Setting">
    <LineStart>#define</LineStart>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#969697</Foreground>
    <FontWeight>Normal</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightLineRule>
  <HighlightLineRule name="Content">
    <LineStart>$</LineStart>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#A31515</Foreground>
    <FontWeight>Normal</FontWeight>
    <FontStyle>Normal</FontStyle>
  </HighlightLineRule>
  <AdvancedHighlightRule name="Numbers">
    <Expression>\b([0-9]+)\b</Expression>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#F68A1B</Foreground>
    <FontWeight>Normal</FontWeight>
    <FontStyle>Normal</FontStyle>
  </AdvancedHighlightRule>
  <AdvancedHighlightRule name="String">
    <Expression>\"(\\\\\"|\\\\\\\\|\\\\n|[^\"])*\"</Expression>
    <IgnoreCase>false</IgnoreCase>
    <Foreground>#A31515</Foreground>
    <FontWeight>Normal</FontWeight>
    <FontStyle>Normal</FontStyle>
  </AdvancedHighlightRule>
</Syntax>
